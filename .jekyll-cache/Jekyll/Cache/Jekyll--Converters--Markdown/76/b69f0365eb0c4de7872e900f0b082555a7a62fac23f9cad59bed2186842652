    <span class="nl">font-family</span><span class="p">:</span> <span class="s1">'Y_Spotlight'</span><span class="p">;</span>
    <span class="nl">src</span><span class="p">:</span> <span class="sx">url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts-20-12@1.0/Y_Spotlight.woff')</span> <span class="nf">format</span><span class="p">(</span><span class="s1">'woff'</span><span class="p">);</span>
